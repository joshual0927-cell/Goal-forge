{
  "version": 2,
  "builds": [
    {
      "src": "api/plan.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "thanks.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/plan",
      "dest": "/api/plan.js"
    },
    {
      "src": "/thanks",
      "dest": "/thanks.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
